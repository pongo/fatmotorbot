{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-eslint-rules",
    "tslint-microsoft-contrib",
    "tslint-consistent-codestyle",
    "tslint-sonarts",
    "typestrict",
    "tslint-no-circular-imports"
  ],
  "rules": {
    "completed-docs": false,
    "curly": [true, "ignore-same-line"],
    "cyclomatic-complexity": [true, 15],
    "early-exit": [
      true,
      {
        "ignore-constructor": true,
        "max-length": 4
      }
    ],
    "export-name": false,
    "function-name": false,
    "import-name": false,
    "increment-decrement": [true, "allow-post"],
    "linebreak-style": false,
    "match-default-export-name": false,
    "max-file-line-count": [true, 400],
    "max-func-body-length": [
      true,
      75,
      {
        "ignore-parameters-to-function-regex": "^describe$"
      }
    ],
    "max-line-length": false,
    "member-access": [true, "no-public"],
    "member-ordering": false,
    "missing-jsdoc": false,
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-accessor-recursion": true,
    "no-backbone-get-set-outside-model": false,
    "no-commented-code": false,
    "no-console": false,
    "no-else-after-return": true,
    "no-implicit-dependencies": [true, "dev", ["~", "src", "test"]],
    "no-increment-decrement": false,
    "no-inferrable-types": false,
    "no-inferred-empty-object-type": false,
    "no-null-keyword": false,
    "no-parameter-properties": false,
    "no-relative-imports": false,
    "no-reserved-keywords": false,
    "no-single-line-block-comment": false,
    "no-static-this": true,
    "no-submodule-imports": [true, "~", "src", "nanoid/generate", "test"],
    "no-suspicious-comment": false,
    "no-typeof-undefined": true,
    "no-unnecessary-else": true,
    "no-unnecessary-type-annotation": [true, "check-return-type"],
    "no-unused-expression": [true, "allow-new"],
    "no-unused-variable": false,
    "no-use-before-declare": false,
    "no-var-requires": false,
    "no-void-expression": [true, "ignore-arrow-function-shorthand"],
    "object-literal-sort-keys": false,
    "object-shorthand-properties-first": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "any"
      }
    ],
    "prefer-array-literal": false,
    "prefer-type-cast": false,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "never"
      }
    ],
    "strict-boolean-expressions": [
      true,
      "allow-null-union",
      "allow-undefined-union",
      "allow-mix",
      "allow-boolean-or-undefined",
      "ignore-rhs"
    ],
    "trailing-comma": false,
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "typedef": [true, "property-declaration"],
    "variable-name": [true, "ban-keywords"],
    "chai-vague-errors": false,
    "quotemark": false,
    "no-shadowed-variable": false
  }
}
